package exam0104;

import java.util.Scanner;

public class Exam6 {

	public static void main(String[] args) {
		int sum;
		  sum=0;
		  System.out.println("5의 배수를 뺀 1에서 1000까지의 합 구하기");
		  
		  for(int i=1;  i<=1000;  i++)
		   {
		   if(i%5==0) continue;
		    sum+=i;
		    }
		     System.out.println("5의 배수를 제외한합은"+ sum + "입니다.");

	}

}
